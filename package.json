{
  "name": "react-instagram-stories",
  "version": "1.0.0",
  "description": "High-performance, fully customizable Instagram-style Stories component for React with TypeScript support",
  "author": "Ankit Jangir",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ankit64jangir/react-instagram-stories"
  },
  "homepage": "https://github.com/ankit64jangir/react-instagram-stories#readme",
  "bugs": {
    "url": "https://github.com/ankit64jangir/react-instagram-stories/issues"
  },
  "keywords": [
    "react",
    "stories",
    "instagram",
    "instagram-stories"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build:lib": "tsup",
    "build:demo": "vite build",
    "build": "npm run build:lib",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.23",
    "lucide-react": "^0.562.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "tsup": "^8.5.1",
    "typescript": "^5.0.0",
    "vite": "^4.3.0"
  },
  "dependencies": {
    "framer-motion": "^12.26.2",
    "react-router-dom": "^7.10.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "sideEffects": [
    "*.css"
  ]
}
